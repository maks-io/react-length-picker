{
  "name": "react-length-picker",
  "version": "3.0.2",
  "description": "A simple length picker that works with metric and imperial units",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "keywords": [
    "react",
    "length",
    "cm",
    "inches",
    "feet",
    "measurements",
    "centimeters",
    "meters",
    "height"
  ],
  "scripts": {
    "clear-build-folder": "rm -rf ./build",
    "transpile": "tsc",
    "fix-paths": "tsc-alias",
    "build": "yarn clear-build-folder && yarn transpile && yarn fix-paths",
    "dev": "yarn nodemon",
    "depcheck": "npx depcheck --config .depcheckrc",
    "release": "yarn && yarn build && npx depcheck && npm publish"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maks-io/react-length-picker.git"
  },
  "author": "Markus Kurzmann <markus@kurzmann.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maks-io/react-length-picker/issues"
  },
  "homepage": "https://github.com/maks-io/react-length-picker#readme",
  "dependencies": {
    "lodash.range": "^3.2.0",
    "virtuosa": "^1.0.2"
  },
  "devDependencies": {
    "@types/lodash.range": "^3.2.9",
    "@types/react": "~18.3.3",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "react": "18.3.1",
    "tsc": "^2.0.4",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.2"
  }
}
